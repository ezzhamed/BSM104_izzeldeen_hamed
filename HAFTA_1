public static void main(String[] args) {
   
   System.out.println(digitRange(68437));
 
}
 
 public static int digitRange(int n ) {
  int min = n % 10 ;
  int max = n % 10 ;
  
 while(n!=0){
   int a = n%10 ;
   min = Math.min(min, a);
   max = Math.max(max , a);
  
   n/=10;
  }
  
  return max - min ;
}